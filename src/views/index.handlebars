<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>Xayania</title>
  <meta name="description" content="description!" />
  <meta name="google-site-verification" content="haESfl9n2k-TL_02Le4baHKelO3VvW2zV2OS_fm4aHo" />

  <link rel="stylesheet" href="/styles.css">
  <script async src="/jquery.js"></script>
  <script src="/script.js"></script>

</head>


<body>
  <div class="snap">
    {{> topnav }}
    <div class="subheader">
      <h1 id="type"></h1>
    </div>

    <section class="photography">
      <h1>Photography</h1>
      <div class="square-container gallery">
        {{#each photography }}
        <div class="square">
          <div class="galleryContent">
            <picture>
              <source srcset="/file/a/{{ this.FileName }}_Default.jpg 3840w,
                  /file/a/{{ this.FileName }}_High.jpg 2880w,
                  /file/a/{{ this.FileName }}_MediumHigh.jpg 1440w,
                  /file/a/{{ this.FileName }}_Medium.jpg 1920w,
                  /file/a/{{ this.FileName }}_Low.jpg 1080w">
              <img decoding="async" loading="lazy" id="{{this.PostID}}" src="/file/a/{{ this.FileName }}_Low.jpg">
            </picture>
          </div>
        </div>
        {{/each}}
      </div>
    </section>

    <section>
      <h1>3D Art</h1>
    </section>

    <section>
      <h1>Music</h1>
    </section>


    {{> footer }}
  </div>
</body>

</html>